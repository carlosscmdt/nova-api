# Copy this file to .env and fill in your values

# Anthropic AI API Key (required)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Server Port (optional, defaults to 3000)
PORT=3000

# Shopify App Configuration (only needed for Shopify embedded app mode)
SHOPIFY_API_KEY=your_shopify_api_key
SHOPIFY_API_SECRET=your_shopify_api_secret
SCOPES=read_products,write_products,read_themes,write_themes,read_content,write_content,read_files,write_files
HOST=https://your-app-url.com
